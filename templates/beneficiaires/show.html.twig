{% extends 'base.html.twig' %}

{% block title %}Beneficiaire{% endblock %}

{% block body %}
    <h1>Beneficiaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ beneficiaire.id }}</td>
            </tr>
            <tr>
                <th>NomPrenom</th>
                <td>{{ beneficiaire.nomPrenom }}</td>
            </tr>
            <tr>
                <th>DateNaissance</th>
                <td>{{ beneficiaire.dateNaissance ? beneficiaire.dateNaissance|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Lien</th>
                <td>{{ beneficiaire.lien }}</td>
            </tr>
            <tr>
                <th>DateLunette</th>
                <td>{{ beneficiaire.dateLunette ? beneficiaire.dateLunette|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_beneficiaires_index') }}">back to list</a>

    <a href="{{ path('app_beneficiaires_edit', {'id': beneficiaire.id}) }}">edit</a>

    {{ include('beneficiaires/_delete_form.html.twig') }}
{% endblock %}
